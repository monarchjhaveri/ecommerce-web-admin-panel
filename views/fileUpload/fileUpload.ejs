<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
</head>
    <body>
        <h1>File Upload</h1>
        <a href="/pages/fileUpload/status">Recently uploaded files</a>
        <form action="/api/fileUpload" method="post" name="submit" enctype="multipart/form-data">
            <label for="filetype">
                File Type: <br/>
                <label>
                    <input type="radio" name="filetype" value="Archive">
                    Archive - <a href="/csv_templates/Archive.csv">Download Template</a>
                </label><br/>
                <label>
                    <input type="radio" name="filetype" value="Inventory">
                    Inventory - <a href="/csv_templates/Inventory.csv">Download Template</a>
                </label><br/>
                <label>
                    <input type="radio" name="filetype" value="MerchantSKUs" checked>
                    MerchantSKUs - <a href="/csv_templates/MerchantSKUs.csv">Download Template</a>
                </label><br/>
                <label>
                    <input type="radio" name="filetype" value="Price">
                    Price - <a href="/csv_templates/Price.csv">Download Template</a>
                </label><br/>
                <label>
                    <input type="radio" name="filetype" value="Variation">
                    Variation - <a href="/csv_templates/Variation.csv">Download Template</a>
                </label><br/>
            </label><br/>
            <input type="file" name="file"><br/>
            <input type="submit" name="submit" value="submit">
        </form>
    </body>
</html>